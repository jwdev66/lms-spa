{
    "name": "olomi/wazo",
    "type": "project",
    "description": "A capacity building platform.",
    "keywords": [
        "idea",
        "wazo",
        "laravel",
        "project"
    ],
    "license": "Apache License",
    "require": {
        "php": "^7.2",
        "brexis/laravel-workflow": "1.3.3",
        "doctrine/dbal": "2.9.2",
        "fideloper/proxy": "4.2.1",
        "infyomlabs/coreui-templates": "6.0.x-dev",
        "infyomlabs/laravel-generator": "6.0.x-dev",
        "laravel/framework": "6.0.4",
        "laravel/socialite": "4.2.0",
        "laravel/tinker": "1.0.10",
        "laravelcollective/html": "6.0.2",
        "owen-it/laravel-auditing": "9.3.2",
        "sentry/sentry-laravel": "1.4.1",
        "spatie/laravel-medialibrary": "7.14.0",
        "spatie/laravel-permission": "3.0",
        "tymon/jwt-auth": "1.0.0-rc.5",
        "yajra/laravel-datatables-buttons": "4.8.0",
        "yajra/laravel-datatables-html": "4.19.0",
        "yajra/laravel-datatables-oracle": "9.6.0"
    },
    "require-dev": {
        "appointer/swaggervel": "2.5.0",
        "barryvdh/laravel-ide-helper": "2.6.6",
        "filp/whoops": "2.5.0",
        "fzaninotto/faker": "1.9.0",
        "infyomlabs/swagger-generator": "dev-master",
        "laravel/dusk": "5.6.3",
        "mockery/mockery": "1.3.0",
        "nunomaduro/collision": "3.0.1",
        "phpunit/phpunit": "8.5.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
